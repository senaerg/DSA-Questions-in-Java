class Solution {
    public boolean isSymmetric(TreeNode root) {
        return isBinSymmetic(root.left, root.right);
    }
    private boolean isBinSymmetic(TreeNode r1, TreeNode r2){

        if(r1==null&&r2==null) return true;
        if(r1==null||r2==null||r1.val!=r2.val) return false;
        return isBinSymmetic(r1.left,r2.right)&&isBinSymmetic(r1.right,r2.left);
    }
}
